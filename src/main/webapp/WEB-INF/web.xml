<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>Startup</servlet-name>
		<display-name>Startup</display-name>
		<description></description>
		<servlet-class>com.example.servlet.Startup</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BasketPage</servlet-name>
		<display-name>BasketPage</display-name>
		<description></description>
		<servlet-class>com.example.servlet.BasketPage</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Startup</servlet-name>
		<url-pattern>/Home</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BasketPage</servlet-name>
		<url-pattern>/addProduct</url-pattern>
	</servlet-mapping>
</web-app>
